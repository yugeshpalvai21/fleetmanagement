<%= form_with(model: @ticket, url: tickets_path, local: true) do |form| %>
  <%= form.label :subject %>
  <%= form.text_field :subject %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.label :status %>
  <%= form.text_field :status %>

  <%= form.submit %>
<% end %>
