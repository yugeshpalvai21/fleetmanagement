<div class="container">
  <h3 align="center">Avg Odometer Report By Nationality</h3>
  <hr>
  <div class="text-right">
    <%= link_to "Download Report as CSV", odometer_report_vehicles_path(format: :csv), class: 'btn btn-info btn-lg' %>
  </div>
  <table class="table">
    <thead>
      <!-- Add the table content here -->
    </thead>
    <tbody>
      <% @report_data.each do |key, value| %>
        <tr>
          <td><%= key.upcase %></td>
          <td><%= value %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
