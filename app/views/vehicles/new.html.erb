<div class="container">
    <h3>Upload CSV Data</h3><hr>
    <%= form_with url: '/vehicles', multipart: true do |form| %>
        <div class="form-group">
            <%= form.file_field :csv_data %>
        </div>
        <div class="form-group">
            <%= form.submit "Upload", class: 'btn btn-lg btn-primary' %>
        </div>
    <% end %>
</div>